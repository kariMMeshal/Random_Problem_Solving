#include <iostream>
#include <bits/stdc++.h>
#define fast ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
typedef long long ll;

int main() {
    fast;

    int t;
    cin >> t;
    while (t--) {
        bool skipOuter = false;
        int n;
        cin >> n;
        vector<int> arr(200001);
        for (int i = 1; i <= n; i++) {
            cin >> arr[i];
        }

        if (arr[1] > n) {
            cout << "NO\n";
            continue;
        }
        for (int i = 1; i <= n; i++) {
            if (arr[arr[i]] < i) {
                cout << "NO\n";
                skipOuter = true;
                break;
            }
        }
        if (!skipOuter) cout << "YES\n";
    }
    return 0;
}


